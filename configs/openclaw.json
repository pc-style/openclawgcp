{
  "meta": {
    "lastTouchedVersion": "2026.2.3"
  },
  "browser": {
    "enabled": true,
    "executablePath": "/usr/bin/chromium",
    "headless": true,
    "noSandbox": true,
    "defaultProfile": "default",
    "profiles": {
      "default": {
        "cdpPort": 9222,
        "color": "#4285F4"
      }
    }
  },
  "models": {
    "providers": {
      "proxypal": {
        "baseUrl": "http://host.docker.internal:8317/v1",
        "apiKey": "YOUR_CLIPROXY_API_KEY",
        "api": "openai-completions",
        "models": [
          {
            "id": "claude-opus-4-6-thinking",
            "name": "Claude Opus 4.6 Thinking",
            "reasoning": false,
            "input": ["text", "image"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 200000,
            "maxTokens": 8192
          },
          {
            "id": "gemini-claude-sonnet-4-5",
            "name": "Claude Sonnet 4.5",
            "reasoning": false,
            "input": ["text", "image"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 200000,
            "maxTokens": 8192
          },
          {
            "id": "gemini-claude-sonnet-4-5-thinking",
            "name": "Claude Sonnet 4.5 Thinking",
            "reasoning": false,
            "input": ["text", "image"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 200000,
            "maxTokens": 8192
          },
          {
            "id": "gemini-3-pro-preview",
            "name": "Gemini 3 Pro",
            "reasoning": false,
            "input": ["text", "image"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 200000,
            "maxTokens": 8192
          },
          {
            "id": "gemini-3-flash-preview",
            "name": "Gemini 3 Flash",
            "reasoning": false,
            "input": ["text", "image"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 200000,
            "maxTokens": 8192
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "proxypal/gemini-claude-sonnet-4-5",
        "fallbacks": []
      },
      "imageModel": {
        "primary": "proxypal/gemini-3-pro-preview"
      },
      "contextPruning": {
        "mode": "cache-ttl",
        "ttl": "1h"
      },
      "compaction": {
        "mode": "safeguard"
      },
      "heartbeat": {
        "every": "30m"
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      },
      "sandbox": {
        "browser": {
          "enabled": true
        }
      }
    },
    "list": [
      {
        "id": "main",
        "default": true,
        "name": "Javis",
        "model": "proxypal/claude-opus-4-6-thinking"
      },
      {
        "id": "lena",
        "name": "Lena",
        "model": "proxypal/gemini-3-pro-preview"
      },
      {
        "id": "marcus",
        "name": "Marcus",
        "model": "proxypal/gemini-3-pro-preview"
      }
    ]
  },
  "tools": {
    "deny": ["zalouser"],
    "agentToAgent": {
      "enabled": true,
      "allow": ["main", "lena", "marcus"]
    }
  },
  "bindings": [
    {
      "agentId": "main",
      "match": { "channel": "telegram", "accountId": "javis" }
    },
    {
      "agentId": "lena",
      "match": { "channel": "telegram", "accountId": "lena" }
    },
    {
      "agentId": "marcus",
      "match": { "channel": "telegram", "accountId": "marcus" }
    },
    {
      "agentId": "marcus",
      "match": { "channel": "zalox", "accountId": "default" }
    }
  ],
  "messages": {
    "ackReactionScope": "group-mentions"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto",
    "restart": true
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "customCommands": [
        { "command": "deal", "description": "üî• Xem deal hot h√¥m nay" },
        { "command": "seller", "description": "üè™ H∆∞·ªõng d·∫´n tr·ªü th√†nh seller" },
        { "command": "price", "description": "üí∞ Xem b·∫£ng ph√≠ platform" },
        { "command": "web", "description": "üåê Link website keothom.bid" },
        { "command": "help", "description": "‚ùì Tr·ª£ gi√∫p & h∆∞·ªõng d·∫´n" }
      ],
      "dmPolicy": "pairing",
      "groupPolicy": "open",
      "streamMode": "partial",
      "accounts": {
        "javis": {
          "dmPolicy": "pairing",
          "botToken": "YOUR_JAVIS_BOT_TOKEN",
          "groupPolicy": "allowlist",
          "streamMode": "partial"
        },
        "lena": {
          "dmPolicy": "pairing",
          "botToken": "YOUR_LENA_BOT_TOKEN",
          "groupPolicy": "allowlist",
          "streamMode": "partial"
        },
        "marcus": {
          "dmPolicy": "pairing",
          "botToken": "YOUR_MARCUS_BOT_TOKEN",
          "groupPolicy": "allowlist",
          "streamMode": "partial"
        }
      }
    },
    "zalouser": {
      "enabled": false,
      "dmPolicy": "open",
      "accounts": {
        "JackShrimp": {
          "enabled": true,
          "profile": "default",
          "dmPolicy": "open",
          "allowFrom": ["YOUR_ZALO_USER_ID"]
        }
      },
      "retryOnClose": true
    },
    "zalox": {
      "enabled": true,
      "dmPolicy": "allowlist",
      "accounts": {
        "default": {
          "enabled": true,
          "dmPolicy": "allowlist",
          "allowFrom": ["YOUR_ZALO_USER_ID"]
        }
      }
    }
  },
  "gateway": {
    "mode": "local",
    "controlUi": {
      "allowInsecureAuth": true
    },
    "trustedProxies": [
      "172.18.0.1",
      "172.16.0.0/12",
      "10.0.0.0/8",
      "192.168.0.0/16",
      "127.0.0.1"
    ]
  },
  "plugins": {
    "load": {
      "paths": [
        "/home/node/.openclaw/extensions/moltbrain",
        "/home/node/.openclaw/workspace/zalox/plugin"
      ]
    },
    "entries": {
      "telegram": { "enabled": true },
      "moltbrain": {
        "enabled": true,
        "config": {
          "autoCapture": true,
          "maxMemories": 10,
          "dataDir": ".moltbrain"
        }
      },
      "zalouser": { "enabled": false },
      "zalox": { "enabled": true }
    }
  }
}
